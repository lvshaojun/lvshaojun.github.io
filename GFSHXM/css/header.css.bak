body {margin:0px;}
ul,li,p ,span,h2{padding:0;margin:0;list-style:none;}
img {vertical-align:middle;}
input,button {padding:0;outline:none;border:none;margin:0;}
 .clear:after {
		content:"";
		display:block;
		clear:both;
	}
	a {text-decoration:none;color:#000;}
.content {
	width:1226px;
	margin:0 auto;
	
}
.nav {
	width:100%;
	height:40px;
	background:#333333;
	line-height:40px;
}
.nav .list {
	float:left;
	color:white;
	font-size:12px;
	color:#ccc;
	/* position:relative; */

}
.nav .list li .app {
	width:124px;
	height:150px;
	background:#fff;

	position:absolute;
	left:480px;top:40px;
	z-index:2;
	display:none;
	

}
.nav .list li .app i {
	border-top:10px solid transparent;
	border-right:10px solid transparent;
	border-bottom:10px solid #fff;
	border-left:10px solid transparent;
	position:absolute;
	top:-20px;
	left:50px;
}

.nav .list li .app img {
    display:block;
	width:86px;
	height:86px;
	margin:20px auto 0;
}
.nav .list li .app span {
	display:block;
	width:104px;
	font-size:14px;
	text-align:center;
}
.nav .list li:hover .app {
	display:block;
	box-shadow:0 0 20px rgba(0,0,0,0.6);
}
.nav .list1 {
	float:right;
	color:white;
	font-size:12px;
	color:#ccc;
}
.list li {
	float:left;
}
.list1 li {
	float:left;
}
.list span {
	float:left;
	margin:0 8px;
	color:#423d39;
}
.nav .list1 span {
	float:left;
	margin:0 8px;
	color:#423d39;
}
.nav li:hover {
	color:white;
}
.nav2 .content {
	height:100px;
	line-height:100px;
	position:relative;
}
.nav2 .content .logo {
	margin:auto 0;
}
.nav2 .shopNav {
	width:1226px;
	height:228px;
	background:#fff;
	border-top:1px solid #708090;
	
	position:absolute;
	left:0px;top:100px;
	display:none;
	z-index:10;
	}
.nav2 .shopNav .shop .box {
	width:203px;
	height:228px;
	float:left;
	font-size:12px;
