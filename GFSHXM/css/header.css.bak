	text-align:center;
	line-height:20px;
	background:red;
}
.nav2 li:hover .shopNav {
	display:block;
}
.nav2 .logo {
	height:78px;
	float:left;
	margin-top:22px;
}
.nav2 .smallNav {
	float:left;
	width:850px;
	margin-left:189px;
		
}
.nav2 .content .search {
	width:294px;
	height:48px;
	border:1px solid #e0e0e0;
	
	position:absolute;
	left:930px;
	top:24px;
}
.nav2 .content .search .search-1 {
	width:243px;
	height:48px;

	position:relative;
}
.nav2 .content .search .search-1 input {
	width:43px;
	height:48px;
	position:absolute;
	 left:0px;top:0px;
	 padding-left:200px;
	 border-right:1px solid #ccc;
}
.nav2 .content .search .bg1 {
	
	position:absolute;
	left:255px;top:12px;
}
.nav2 .content .search .bg2 {
	display:none;
	position:absolute;
	left:255px;top:12px;
}
.nav2 .content .search:hover {
	background:#ff6700;
}
.nav2 .content .search:hover .bg2{
	display:block;
}
.nav2 .smallNav li {
	height:100px;
