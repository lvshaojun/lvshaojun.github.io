	position:relative;
}
.nav2 .shopNav .shop .box img {
	width:160px;
	height:110px;
	display:block;
	margin:15px auto 0;
	border-left:1px solid #e0e0e0;
}
.nav2 .shopNav .shop .box a {
	display:block;
