/* 头部 */
#head {
	width:100%;
}
#head .top {
	width:100%;
	border-bottom:1px solid #e8e8e8;
}
.top-p {
	width:1230px;
	margin:0 auto;
}
.top-p .topleft {
	width:250px;
	float:left;
}
.top .topleft a {
	height:32px;
	line-height:32px;
	font-size:11px;
	color:#afa5ba;
	margin-right:20px;
	
}
.topleft a .left3 {
	margin-right:0px;
}
.top-p .topright {
	width:600px;
	float:right;
	position:relative;
}
.top-p .topright div {
	float:right;
	height:32px;
	line-height:32px;
	font-size:11px;
	color:#afa5ba;
	margin-left:40px;
}
.topright a:hover {
	color:red;
	text-decoration:underline;
}
.right7-box {
	width:100px;
	background:red;
	position:absolute;
	top:32px;
	left:30px;
	display:none;
}
.right7:hover .right7-box{
	display:block;
}
#head .content {
	width:100%;
	height:128px;
	background:#fff;
	border-bottom:1px solid #ff0036;
	position:relative;
}
.content .content-t {
	width:1230px;
	margin:0 auto;
}
.content-top .nav {
	position:relative;
}
.content-top .nav .img1{
	position:absolute;
	top:-35px;
	left:0px;
}
.nav .nav-top {
	width:625px;
	height:36px;
	margin:38px auto;
	border-top:1px solid #ff0036;
	border-right:1px solid #ff0036;
	border-left:2px solid #ff0036;
	border-bottom:2px solid #ff0036;
}
.nav-top input {
	width:481px;
	font-size:14px;
	height:36px;
	line-height:36px;
	margin-left:10px;
}
.nav-top img {
	width:131px;
	float:right;
}
.nav-bottom {
	height:30px;
	width:625px;
	margin:-36px auto;
}
.nav-bottom div {
	float:left;
	width:66px;
	height:30px;
	text-align:center;
	margin-left:-5px;
}
.nav-bottom .btm2,.btm5,.btm7 {
	border-left:1px solid #dbdbdb;
	border-right:1px solid #dbdbdb;
}
.nav-bottom a {
	font-size:14px;
	height:30px;
	line-height:30px;
	color:#999;
}
.btm2 a {color:#ff0036;}
.btm4 a {color:#ff0036;}
.btm7 a {color:#ff0036;}
.btm9 a {color:#ff0036;}
.content-bottom {
	width:1014px;
	height:35px;
	position:absolute;
	bottom:0;
}
.content-bottom .list1 {
	background:red;
	height:35px;
	width:200px;
	position:relative;
}
.content-bottom .list1 .nav {
	width:200px;
	height:500px;
	background:rgba(0,0,0,0.6);
	position:absolute;
	top:35px;
	z-index:1;
}
.navlist li {

	height:31px;
	line-height:31px;
}
.navlist a{
	height:32px;
	font-size:14px;
	color:#fff;
}
.navlist p {
	padding-left:40px;
	vertical-align:middle;
	
	color:#fff;
	background:url("../img/未标题-6.gif") no-repeat -5px;
}
.list1 .imgp {
	position:absolute;
	top:10px;
	left:10px;
}
.list1 h4 {
	font-size:15px;
	float:left;
	color:#fff;
	vertical-align:middle;
	margin:-3px 0 0 10px;
}
.list1 img {
	float:left;
}
.list1 {
	float:left;
}
.list2 {
	float:left;
}
.list3 {
	float:left;
	font-size:0;
}
.list3 a {
	height:35px;
	line-height:35px;
	font-size:15px;
	margin-left:16px;
}
.list3 a:hover {
	color:#ff0036;
}
.navright {
	width:850px;
	height:500px;
	background:#fff;
	position:absolute;
	left:200px;
	top:0;
	display:none;
}
.navright .nav-lt {
	width:630px;
	height:500px;
	float:left;
}
.nav-lt .lt-1 {
	width:120px;
	float:left;
}
.nav-lt .lt-1 li {
	font-size:15px;
	height:52px;
	text-align:center;
	font-weight:bold;
	padding-top:10px;
}
.nav-lt .lt-2 {
	width:500px;
	float:left;
}
.nav-lt .lt-2 li a {
	color:#000;
	margin-left:5px;
	font-size:13px;
}
.nav-lt .lt-2 li a:hover {
	color:red;
}
.nav-lt .lt-2 li {
	height:56px;
	margin-top:4px;
	border-bottom:1px dashed #000;
}

.navright .nav-rt {
	width:200px;
	height:480px;
	float:right;
	border:10px solid #f5f5f5;
}
.navright .nav-rt img {
	width:94px;
	height:115px;
	margin:2px;
	border:1px solid #f5f5f5;
}
.navlist li:hover {
	background:#fff;
}
li:hover a {color:rgb(229,64,119);}
li:hover span {color:rgb(229,64,119);}
.navlist li:hover .navright {
	display:block;
}
